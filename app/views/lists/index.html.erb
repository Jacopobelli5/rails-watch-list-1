<h1>Your Movies</h1>
<div class="container py-5">
  <div class="d-flex align-items-center justify-content-between mb-4">
  <div class="container">
   <h3>My Lists</h3>
  </div>

  </div>
  <div class="row">
    <% @lists.each do |list| %>
      <div class="co-12 col-sm-6 mb-3">
        <%= link_to list_path(list), class:"text-decoration-none" do %>
          <div class="card-category">
            <%= list.name %>
          </div>
        <% end %>
  </div>
    <% end %>

  <div class="container">
    <%= link_to "Create a movie list", new_list_path, class: "btn btn-primary" %>
  </div>
